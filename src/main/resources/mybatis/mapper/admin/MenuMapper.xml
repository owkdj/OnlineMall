<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.jdkwo.dao.admin.MenuDao">
    <!--根据用户名查询用户-->
    <insert id="addMenu" parameterType="Menu">
        INSERT INTO menu(parentId,name,url,icon) VALUES(#{parentId},#{name},#{url},#{icon})
    </insert>

</mapper>